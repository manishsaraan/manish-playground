{
  "name": "ansible-client-portal",
  "version": "1.0.0",
  "description": "Ansible client portal that renders user workspace and dashboards",
  "main": "server.js",
  "scripts": {
    "start:dev": "webpack-dev-server --mode development",
    "start": "NODE_ENV=production node server.js",
    "build": "webpack --mode production",
    "test": "npm-run-all --silent test-*",
    "test-nsp": "nsp check",
    "test-lint": "eslint ./*.js find ./config ./controllers ./routes ./services ./src ./test '*.js'",
    "test-mocha": "find ./test -name '*.js' | xargs nyc mocha --recursive --reporter mochawesome --reporter-options reportDir=./logs ./test/test-setup.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/coreplatformteam/webclientportal.git"
  },
  "author": "Ansible Platform Developers",
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "ejs": "^2.5.8",
    "express": "^4.16.3",
    "nsp": "^3.2.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "redux-thunk": "^2.2.0",
    "winston": "^2.4.1"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-react-transform": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.18",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.4",
    "jsdom": "11.7.0",
    "jsdom-global": "3.0.2",
    "md5": "^2.2.1",
    "mocha": "^5.0.4",
    "mochawesome": "^3.0.2",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "sass-loader": "^6.0.7",
    "sinon": "^4.5.0",
    "style-loader": "^0.20.3",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  }
}
